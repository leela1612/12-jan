int main()  
{  int arr[10];
   for (int i = 0; i < n ; i++)
       {
           std::cin>>arr[i];
       }
   int range1=0;
   int a=100;
   for (int i = 0; i < n ; i++)
       {
           if (a>=arr[i])
           { a=arr[i];
               range1=i;
           }
       } //min element
    a=0
    int range2=0;
    for (int i = 0; i < n ; i++)
       {
           if (a=<arr[i])
           { a=arr[i];
               range2=i;
           }
       }
       
       
       
}
